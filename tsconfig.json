{
	"compilerOptions": {
		"target": "ES6",
		"outDir": "build",
		"module": "commonjs",
		"strict": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitReturns": false,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules"]
}
